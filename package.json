        "mocha": "^5.1.1",
        "nodemon": "^1.17.3",
        "nyc": "^11.7.1",
        "sendmail": "^1.4.1",
        "ts-node": "^6.0.0",
        "tsc-watch": "^1.0.22",
        "typescript": "^2.8.3",
        "typescript-eslint-parser": "^15.0.0"
    },
