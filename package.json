{
    "name": "kenry-nice",
    "version": "1.0.0",
    "description": "Backend environment",
    "scripts": {
        "watch:build": "tsc --watch",
        "watch:server": "NODE_ENV=Development SINGLE_THREAD=true nodemon './dest/server.js' --watch './dest'",
        "watch": "npm-run-all --parallel watch:build watch:server --print-label",
        "eslint": "./node_modules/.bin/eslint --ext .ts --ext .js ./src",
        "build": "npm run eslint && ./node_modules/.bin/grunt sync && ./node_modules/.bin/tsc",
        "start": " npm run build && NODE_ENV=Development node ./dest/server.js",
        "staging": "NODE_ENV=Staging node ./dest/server.js",
        "production": "NODE_ENV=Production node ./dest/server.js"
    },
    "dependencies": {
        "amqplib": "^0.5.2",
        "babel-eslint": "^8.2.3",
        "cors": "^2.8.4",
        "cron": "^1.3.0",
        "email-existence": "^0.1.6",
        "eventstore": "^1.14.1",
        "express": "^4.16.3",
        "fast-csv": "^2.4.1",
        "grunt-contrib-watch": "^1.1.0",
        "minimatch": "^3.0.4",
        "moment": "^2.22.2",
        "mongodb": "3.0.8",
        "mongoose": "^5.0.15",
        "morgan": "^1.9.0",
        "multer": "^1.3.1",
        "nedb": "^1.8.0",
        "node-uuid": "^1.4.8",
        "nodemailer": "^4.6.4",
        "npm-run-all": "^4.1.3",
        "path": "^0.12.7",
        "reflect-metadata": "^0.1.12",
        "request": "^2.85.0",
        "request-promise": "^4.2.2",
        "routing-controllers": "^0.7.7",
        "socket.io": "^2.1.1",
        "typeorm": "^0.2.7",
        "validator": "^9.4.1"
    },
    "devDependencies": {
        "@types/chai": "^4.1.3",
        "@types/cron": "^1.3.0",
        "@types/express": "^4.11.1",
        "@types/mocha": "^5.2.0",
        "@types/mongoose": "^5.0.10",
        "@types/morgan": "^1.7.35",
        "@types/nedb": "^1.8.5",
        "@types/node": "^9.6.22",
        "@types/nodemailer": "^4.6.0",
        "chai": "^4.1.2",
        "eslint": "^4.19.1",
        "eslint-config-google": "^0.9.1",
        "fs-extra": "^6.0.1",
        "grunt": "^1.0.3",
